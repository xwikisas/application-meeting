    <property>
      <code>{{velocity}}
#set($discard = $xwiki.ssx.use('Meeting.Code.NextMeetingsMacro'))
#set($limit = $xcontext.macro.params.limit)
#set($nowDate = $datetool.date)

#set($displayOnGoingMeetings = $xcontext.macro.params.displayOnGoingMeetings)
#set ($queryDateType = 'endDate')
#if ($displayOnGoingMeetings == 'false')
  #set ($queryDateType = 'startDate')
#end

#set($query = "from doc.object(Meeting.Code.MeetingClass) as meeting where doc.fullName &lt;&gt; 'Meeting.Code.MeetingTemplate' and meeting.${queryDateType} &gt; :date and meeting.status in ('value1','value2') order by meeting.startDate asc")
#set($results = $services.query.xwql($query).setLimit($limit).bindValue('date', $nowDate).execute())
##
(% class="meetings-next" %)(((
  #foreach($item in $results)
    #set($docReference = $services.model.resolveDocument($item))
    #set($document = $xwiki.getDocument($item))
    #set($startDate = $document.getValue('startDate'))
    #if($services.security.authorization.hasAccess('view', $docReference))
      (% class="list-unstyled" %)
        #if($xcontext.macro.params.status == 'show')
          #set($status = "- $services.localization.render('contrib.meeting.field.status') : $document.display('status', 'view')")
        #end
        * $services.icon.render('time')(% class="text-muted" %) [[$document.plainTitle&gt;&gt;$item]] $document.display('startDate','view') $!status #if ($startDate.compareTo($nowDate) &lt; 0)| {{translation key="meeting.panel.activeMeeting"/}}#end
    #end
  #end
)))
##
#if($results.size() == 0)
  $services.localization.render('contrib.meeting.panel.noMeetings')
#end

#if($xcontext.macro.params.homelink == 'show')
  #if($results.size() == 0)
    #set($spaceReference = $services.model.resolveSpace('Meeting'))
    #if($services.security.authorization.hasAccess('edit', $spaceReference))
      [[$services.localization.render('contrib.meeting.panel.addmeeting')&gt;&gt;Meeting.WebHome||class="homelink"]]
    #end
  #else
      [[$services.localization.render('contrib.meeting.panel.viewallmeetings')&gt;&gt;Meeting.WebHome||class="homelink" queryString="view=future"]]
  #end
#end
{{/velocity}}</code>
